{
  "name": "ZanderMCP",
  "version": "1.0.0",
  "description": "Brain-Computer Interface MCP Server for real-time cognitive state monitoring and workload classification",
  "source": "server.py:mcp",
  "environment": {
    "python": ">=3.9",
    "dependencies": [
      "fastmcp>=2.13.0",
      "pyyaml>=6.0",
      "python-dotenv>=1.0.0",
      "sqlalchemy>=2.0.0",
      "asyncpg>=0.29.0",
      "websockets>=12.0",
      "numpy>=1.24.0",
      "scipy>=1.11.0",
      "alembic>=1.13.0"
    ]
  },
  "deployment": {
    "transport": "http",
    "host": "0.0.0.0",
    "port": 8000,
    "env": {
      "LOG_LEVEL": "INFO",
      "POSTGRES_URL": "${POSTGRES_URL}",
      "EDGE_RELAY_API_KEY": "${EDGE_RELAY_API_KEY}"
    }
  }
}
